page {
  padding-bottom: 20rpx;
  padding-top: 266rpx;
}

.fixed-wrap {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  background: #f5f5f5;
}

.search-wrap {
  padding: 34rpx;
  border-top-width: 5rpx;
}

.search-wrap .search_content {
  position: relative;
}

.search-wrap .search_content .icons-more {
  position: absolute;
  top: 50%;
  left: 20rpx;
  transform: translateY(-50%);
}

.search-wrap .search_content input {
  display: block;
  height: 82rpx;
  line-height: 82rpx;
  padding: 0 56rpx;
  box-sizing: border-box;
  background-color: #fff;
  width: 100%;
}

.select-wrap {
  background: white;
  display: -webkit-box;
  -webkit-box-align: center;
  height: 90rpx;
  padding: 0 30rpx;
  color: #92979b;
  border-bottom: 1rpx solid #dddddd;
  box-shadow: 0 0 20rpx 0 rgba(0, 0, 0, 0.05);
}

.select-wrap .flex {
  -webkit-box-flex: 1;
}

.select-wrap .picker-wrap {
  display: -webkit-box;
  -webkit-box-align: center;
  color: #000000;
}

.select-wrap .picker-wrap .picker {
  padding-right: 10rpx;
}

.select-wrap .picker-wrap .arrow {
  width: 0;
  height: 0;
  border-top: 14rpx solid #000000;
  border-left: 12rpx solid transparent;
  border-right: 12rpx solid transparent;
}

.select-wrap .picker-wrap.picker-wrap2 {
  color: #000000;
}

.select-wrap .picker-wrap.picker-wrap2 .arrow {
  border-top-color: #000000;
}

.list-wrap .item {
  background: white;
  margin-bottom: 24rpx;
  padding: 48rpx 34rpx;
  line-height: 50rpx;
  color: #92979b;
  display: -webkit-box;
  -webkit-box-align: center;
}

.list-wrap .item .con {
  -webkit-box-flex: 1;
}

.list-wrap .item .name text {
  font-weight: bold;
  color: #000000;
  font-size: 26rpx;
  padding-right: 10rpx;
}

.list-wrap .item .tanhao {
  width: 26rpx;
  height: 26rpx;
  background: #fd1616;
  border-radius: 50%;
  color: white;
  text-align: center;
  line-height: 26rpx;
  font-size: 18rpx;
}

.filter-wrap {
  padding: 50rpx 30rpx 25rpx;
}

.filter-wrap .btn-reset {
  font-size: 30rpx;
  color: #333333;
  line-height: 80rpx;
  border-radius: 40rpx;
  border: 1rpx solid #cccccc;
  text-align: center;
  margin-top: 10rpx;
}

.filter-wrap .filter-item-wrap {
  padding-bottom: 30rpx;
}

.filter-wrap .filter-item-wrap .filter-item-title {
  font-size: 30rpx;
  color: black;
  padding-bottom: 30rpx;
  line-height: 30rpx;
}

.filter-wrap .filter-item-wrap .switch-wrap {
  display: -webkit-box;
  -webkit-box-align: center;
}

.filter-wrap .filter-item-wrap .switch-wrap text {
  color: #92979b;
  display: block;
  font-size: 24rpx;
  line-height: 60rpx;
  padding-left: 10rpx;
}

.filter-wrap .filter-item-wrap .switch-wrap text.checked {
  color: #dc1d54;
}

.filter-wrap .filter-item-wrap .birthday {
  display: -webkit-box;
  -webkit-box-align: center;
}

.filter-wrap .filter-item-wrap .birthday text {
  display: block;
  margin-right: 20rpx;
  background: white;
  border-radius: 30rpx;
  font-size: 20rpx;
  color: #aaaaaa;
  border: 1rpx solid #cccccc;
  min-width: 120rpx;
  padding: 0 10rpx;
  line-height: 60rpx;
  text-align: center;
  box-sizing: border-box;
}

.filter-wrap .filter-item-wrap .birthday text.active {
  color: #ff5183;
  border-color: #ff5183;
}

.filter-wrap .filter-item-wrap .birthday text:last-child {
  margin-right: 0;
}

.filter-wrap .filter-item-wrap .slider-wrap {
  display: -webkit-box;
  -webkit-box-align: center;
  color: #dc1d54;
  font-size: 24rpx;
  line-height: 60rpx;
}

.filter-wrap .filter-item-wrap .slider-wrap slider {
  width: 500rpx;
}
