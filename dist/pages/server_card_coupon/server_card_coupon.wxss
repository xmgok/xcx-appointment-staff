.search {
  display: -webkit-box;
  -webkit-box-align: center;
  background: #f5f5f5;
  padding: 0 35rpx 35rpx 35rpx;
  box-shadow: inset 0rpx -1rpx 0rpx 0rpx #dddddd;
}

.search .value_wrap {
  height: 82rpx;
  -webkit-box-flex: 1;
  box-sizing: border-box;
  padding: 0 20rpx;
  display: -webkit-box;
  -webkit-box-align: center;
  background: #fff;
  color: #aaaaaa;
}

.search .value_wrap input {
  display: block;
  -webkit-box-flex: 1;
  box-sizing: border-box;
  padding-left: 20rpx;
  font-size: 28rpx;
  line-height: 82rpx;
  height: 82rpx;
}

.search .cancel {
  padding-left: 15rpx;
  color: #9e925f;
  font-size: 28rpx;
  margin-left: 15rpx;
  display: none;
}

.search .cancel.show {
  display: block;
}

.tec_tab {
  display: flex;
  line-height: 106rpx;
  text-align: center;
  background: #fff;
  position: relative;
  box-shadow: inset 0rpx -1rpx 0rpx 0rpx #dddddd;
}

.tec_tab .before {
  height: 4rpx;
  background: transparent;
}

.tec_tab .before.LShift {
  position: absolute;
  bottom: 0;
  left: 0%;
  right: 66.6667%;
  -webkit-transition: right 0.3s cubic-bezier(0.35, 0, 0.25, 1) 0.09s, left 0.3s cubic-bezier(0.35, 0, 0.25, 1);
  transition: right 0.3s cubic-bezier(0.35, 0, 0.25, 1) 0.09s, left 0.3s cubic-bezier(0.35, 0, 0.25, 1);
}

.tec_tab .before.center {
  position: absolute;
  bottom: 0;
  left: 33.333%;
  right: 33.333%;
  -webkit-transition: right 0.3s cubic-bezier(0.35, 0, 0.25, 1) 0.09s, left 0.3s cubic-bezier(0.35, 0, 0.25, 1);
  transition: right 0.3s cubic-bezier(0.35, 0, 0.25, 1) 0.09s, left 0.3s cubic-bezier(0.35, 0, 0.25, 1);
}

.tec_tab .before.RShift {
  position: absolute;
  bottom: 0;
  left: 66.6667%;
  right: 0;
  -webkit-transition: right 0.3s cubic-bezier(0.35, 0, 0.25, 1), left 0.3s cubic-bezier(0.35, 0, 0.25, 1) 0.09s;
  transition: right 0.3s cubic-bezier(0.35, 0, 0.25, 1), left 0.3s cubic-bezier(0.35, 0, 0.25, 1) 0.09s;
}

.tec_tab .before .element {
  display: block;
  margin: 0 auto;
  height: 100%;
  background: #ff5183;
}

.tec_tab .tec_tab_li {
  flex: 1;
  font-size: 28rpx;
  color: #666666;
}

.tec_tab .tec_tab_li.active {
  color: #FF5183;
}

.sc_content {
  display: -webkit-box;
  margin-bottom: 106rpx;
}

.sc_content .server_list {
  width: 152rpx;
  background: #f5f5f5;
  box-shadow: inset -1rpx 0 0 0 #dddddd;
}

.sc_content .server_list .server_list_li {
  width: 100%;
  padding: 24rpx 0;
  box-shadow: inset 0rpx -1rpx 0rpx 0rpx #dddddd;
  text-align: center;
  color: #333333;
  font-size: 24rpx;
}

.sc_content .server_list .server_list_li.active {
  background: #fff;
}

.sc_content .server_list .server_list_li.active .Dueto {
  font-size: 18rpx;
  color: #dc1d54;
}

.sc_content .product_list {
  -webkit-box-flex: 1;
}

.sc_content .product_list .product_list_li {
  padding: 34rpx;
  background: #fff;
  display: -webkit-box;
  -webkit-box-pack: justify;
  font-size: 23rpx;
  position: relative;
  box-shadow: inset 0rpx -1rpx 0rpx 0rpx #dddddd;
}

.sc_content .product_list .product_list_li .out_date {
  padding: 4rpx 8rpx;
  border-radius: 4rpx;
  background: #ccc;
  color: #fff;
  font-size: 22rpx;
  margin-left: 8rpx;
}

.sc_content .product_list .product_list_li .product_name {
  font-size: 28rpx;
  color: #333333;
  margin-bottom: 24rpx;
}

.sc_content .product_list .product_list_li .product_name.runout {
  color: #aaa;
}

.sc_content .product_list .product_list_li .ser_time {
  color: #92979b;
  font-size: 22rpx;
}

.sc_content .product_list .product_list_li .price {
  margin-top: 24rpx;
  color: #92979b;
}

.sc_content .product_list .product_list_li .price .the_price {
  display: inline-block;
  font-size: 22rpx;
}

.sc_content .product_list .product_list_li .price .the_price:first-child {
  margin-right: 16rpx;
}

.sc_content .product_list .product_list_li .price .the_price .text_color {
  color: #FF5183;
}

.sc_content .product_list .product_list_li .price .the_price .text_color.runout {
  color: #aaa;
}

.sc_content .product_list .product_list_li .price .the_price.text_line {
  text-decoration: line-through;
  color: #cccccc;
}

.sc_content .product_list .product_list_li .price .the_price.text_line .text_color {
  color: #cccccc;
}

.sc_content .product_list .product_list_li .product_count {
  height: 40rpx;
  position: absolute;
  right: 34rpx;
  top: 50%;
  transform: translateY(-50%);
}

.sc_content .product_list .product_list_li .product_count .count_image {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  display: inline-block;
  vertical-align: middle;
}

.sc_content .product_list .product_list_li .product_count .count_text {
  font-size: 28rpx;
  color: #333333;
  padding: 0 24rpx;
  vertical-align: middle;
}

.fixed {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 106rpx;
  z-index: 1;
  background-color: #ffffff;
  display: -webkit-box;
  -webkit-box-pack: justify;
  box-shadow: 0 4rpx 23rpx 0 rgba(0, 0, 0, 0.1);
}

.fixed .people {
  margin-left: 36rpx;
  line-height: 106rpx;
  color: #92979b;
  font-size: 28rpx;
}

.fixed .people .number {
  font-size: 35rpx;
  color: #f96868;
  display: inline-block;
}

.fixed .submit {
  line-height: 106rpx;
  font-size: 35rpx;
  padding: 0 78rpx;
  background: #5D97FF;
  color: #fff;
}
