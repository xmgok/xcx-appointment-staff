.ui-no-data {
  text-align: center;
  padding: 40rpx 0;
  color: #aaaaaa;
}

.ui-dialog-bg {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  z-index: 950;
}

.ui-dialog {
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 960;
  background: white;
  width: 100%;
}

.ui-dialog .dialog-title {
  background: #f5f5f5;
  line-height: 116rpx;
  border-bottom: 1rpx solid #dddddd;
  text-align: center;
  position: relative;
  font-size: 28rpx;
}

.ui-dialog .dialog-title .title {
  font-size: 36rpx;
  font-weight: bold;
}

.ui-dialog .dialog-title .cancel {
  position: absolute;
  left: 0;
  top: 0;
  width: 116rpx;
  color: #92979b;
}

.ui-dialog .dialog-title .confirm {
  position: absolute;
  right: 0;
  top: 0;
  width: 116rpx;
  color: #5D97FF;
}

